{
	"presets":[["@babel/preset-env",{
		"useBuiltIns":"entry"
	}],"@babel/preset-react"],
	"plugins": [
		["@babel/plugin-proposal-class-properties", { "loose": true }],
		[
		  "@babel/plugin-transform-runtime",
		  {
			"absoluteRuntime": false,
			"corejs": false,
			"helpers": true,
			"regenerator": true,
			"useESModules": false
		  }
		]
	  ]
}